{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/carousel/page.tsx", "src/context/themeContext/differentTheme.jsx", "src/utils/authOptions.js", "src/providers/authProvider.tsx", "src/app/api/auth/[...nextauth]/route.js", "src/app/home/page.jsx", "src/app/cards/page.jsx", "src/app/write/page.jsx"],
  "exclude": ["node_modules"]
}
